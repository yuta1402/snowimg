{
  "name": "snowimg",
  "version": "1.0.0",
  "description": "",
  "main": "dist/main.js",
  "scripts": {
    "start": "electron .",
    "build": "webpack --mode production",
    "watch": "webpack --mode development --watch",
    "pack-mac": "electron-packager . --asar --overwrite --platform=darwin --arch=x64",
    "pack-win": "electron-packager . --asar --overwrite --platform=win32 --arch=x64",
    "pack-linux": "electron-packager . --asar --overwrite --platform=linux --arch=x64",
    "package": "npm run pack-mac && npm run pack-win && npm run pack-linux"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yuta1402/snowimg.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/yuta1402/snowimg/issues"
  },
  "homepage": "https://github.com/yuta1402/snowimg#readme",
  "devDependencies": {
    "copy-webpack-plugin": "^6.1.1",
    "css-loader": "^4.3.0",
    "electron": "^9.3.0",
    "electron-packager": "^15.1.0",
    "html-webpack-plugin": "^4.4.1",
    "mini-css-extract-plugin": "^0.11.0",
    "node-sass": "^4.14.1",
    "sass-loader": "^10.0.2",
    "ts-loader": "^8.0.3",
    "typescript": "^4.0.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  }
